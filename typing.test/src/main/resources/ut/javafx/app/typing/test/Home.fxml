<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ut.javafx.app.typing.test.HomeController">
   <children>
      <AnchorPane prefHeight="538.0" prefWidth="794.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="9.0" layoutY="31.0" prefHeight="18.0" prefWidth="775.0" text="Typing Test">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <TextArea fx:id="testGuideLineArea" editable="false" layoutX="14.0" layoutY="94.0" prefHeight="104.0" prefWidth="766.0" text="&#10;1) There will be 20 marks test, one marks for each right character.&#10;2) Character will auto pop up and you have to type the same character from keyboard to score.&#10;3) After 20th attempt, you should compulsory submit your test.&#10;4) You must enter your name and click on start button below to start the test." />
            <TextField fx:id="nameField" layoutX="149.0" layoutY="312.0" promptText="Enter name and click start" />
            <Button fx:id="startTestBtn" layoutX="434.0" layoutY="312.0" mnemonicParsing="false" onAction="#startTest" text="Start Test" />
            <Label fx:id="name" layoutX="82.0" layoutY="311.0" prefHeight="26.0" prefWidth="66.0" text="Name:" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
