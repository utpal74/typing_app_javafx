<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ut.javafx.app.typing.test.ResultController" >
   <children>
      <AnchorPane prefHeight="538.0" prefWidth="794.0">
         <children>
            <VBox alignment="CENTER" layoutX="243.0" layoutY="85.0" prefHeight="405.0" prefWidth="333.0" style="-fx-border-color: orange; -fx-border-width: 5; -fx-padding: 5; -fx-spacing: 20;">
               <children>
                  <HBox alignment="CENTER" prefHeight="53.0" prefWidth="279.0">
                     <children>
                        <Label fx:id="resultLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="39.0" prefWidth="138.0" text="Test result">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="38.0" prefWidth="279.0" style="-fx-spacing: 10;">
                     <children>
                        <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="42.0" prefWidth="173.0" text="You scored:">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <Label fx:id="finalScoreLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="38.0" prefWidth="64.0" text="0">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_CENTER" prefHeight="38.0" prefWidth="279.0" style="-fx-padding: 5; -fx-spacing: 10;">
                     <children>
                        <Button fx:id="retakeTestBtn" mnemonicParsing="false" onAction="#retakeTest" text="Retake Test" />
                        <Button fx:id="closeTestBtn" mnemonicParsing="false" onAction="#closeTest" text="Close Test" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
